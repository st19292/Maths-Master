# Subcomponent 4.2.1: List Exporting

# List Test
answers_list = ['21 + 5 = 26', '11 + 11 = 22', '1 + 13 = 14', '16 + 21 = 37', '7 + 22 = 29',
                '1 + 2 = 3', '13 + 23 = 36', '20 + 22 = 42', '24 + 8 = 32', '18 + 19 = 37']

with open('answers_list.txt', 'w') as file:
    file.write(str(answers_list))
